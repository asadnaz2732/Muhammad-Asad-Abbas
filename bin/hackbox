#!/data/data/com.termux/files/usr/bin/bash
DIR="/data/data/com.termux/files/usr/share"

clear
echo "=================================="
echo "    MY CUSTOM MASTER TOOLBOX      "
echo "=================================="
echo "1) Seeker        5) GhostTrack"
echo "2) CamPhish     6) Storm-Breaker"
echo "3) SayCheese    7) XLR8 Bomber"
echo "4) GrabCam      8) Metasploit"
echo "9) Cloudflared  10) Ngrok"
echo "11) Exit"
echo "=================================="
read -p "Choose a tool: " choice

case $choice in
    1) python3 $DIR/seeker/seeker.py ;;
    2) cd $DIR/CamPhish && bash camphish.sh ;;
    3) cd $DIR/saycheese && bash saycheese.sh ;;
    4) cd $DIR/grabcam && bash grabcam.sh ;;
    5) python3 $DIR/GhostTrack/ghosttrack.py ;;
    6) cd $DIR/Storm-Breaker && python3 st.py ;;
    7) cd $DIR/XLR8_BOMBER && python3 xlr8.py ;;
    8) msfconsole ;;
    9) read -p "Port: " p && cloudflared tunnel --url http://127.0.0.1:$p ;;
    10) read -p "Port: " p && ngrok http $p ;;
    11) exit ;;
    *) echo "Invalid"; sleep 1; hackbox ;;
esac
